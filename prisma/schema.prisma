generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Message {
  id         String   @id @default(uuid())
  created_at DateTime @default(now())
  ParentId   String?
  text       String
  likes      Int      @default(0)
}
